 <%= form_with model: interview, url: public_interviews_path, method: :post do |f| %>
	    <tr>
		    <td>予定日時</td>
		    <td><%= f.datetime_field :schedule %></td>
		</tr>
		<tr>
			<td>タイトル</td>
			<td><%= f.text_field :title %></td>
		</tr>
		<tr>
		    <td>タグ</td>
		    <td><%= f.collection_check_boxes(:tag_ids, Tag.all, :id, :name) do |tag| %>
              　<div>
                 <%= tag.label do %>
                   <%= tag.check_box %>
                   <%= tag.text %>
                 <% end %>
                <% end %>
       　        </div>
       　      </td>
		</tr>
		<tr>
			<td>内容</td>
			<td><%= f.text_area :body, size: "40x15" %></td>
		</tr>
		<tr>
		<tr>
		    <td></td>
		    <td>
               <%= f.submit '確定する' %>
              </td>
		</tr>
	</table>
	 <% end %>